package com.nagarro.adminPanel.utilities;

public class GenerateRandomID {

	public String generateRandomID(String emailID, int contactNumber, int customerName) {

		String firstPart = emailID.substring(0, 3);

		int secondPart = 0;

		for (int i = 0; i < 3; i++) {
			secondPart += contactNumber % 10;
			contactNumber = contactNumber / 10;
		}

	}

}
