package com.nagarro.adminPanel.dao;

import java.util.ArrayList;
import java.util.List;

import javax.persistence.criteria.CriteriaBuilder;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Repository;

import com.nagarro.adminPanel.model.CustomerDetails;

@Repository
public class UserListDaoImpl extends AbstractDAO<String, CustomerDetails> implements UserListDao {

	@Autowired
	SessionDao sessionDao;

	@Override
	public List<CustomerDetails> fetchAllUsers() {
		List<CustomerDetails> listOfAllUsers = new ArrayList<>();

		CriteriaBuilder criteriaBuilder = getSession().beginTransaction();
		
		return listOfAllUsers;

	}
}
